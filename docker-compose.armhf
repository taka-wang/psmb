slave:
    image: takawang/c-modbus-slave:armhf
modbusd:
    image: edgepro/modbusd:armhf
    volumes:
        - /tmp:/tmp
    links:
        - slave
psmbtcp:
    build: tcp/.
    volumes_from:
        - modbusd
dummysrv:
    image: takawang/dummy-srv:armhf
    volumes_from:
        - modbusd
    links:
        - slave

