slave:
    image: takawang/c-modbus-slave:armhf

modbusd:
    image: edgepro/modbusd:armhf
    volumes:
        - /tmp:/tmp
    links:
        - slave

psmbtcpsrv:
    build: tcp-srv/.
    volumes_from:
        - modbusd

dummysrv:
    build: tcp-srv/test/dummy-srv/.
    volumes_from:
        - modbusd
    links:
        - slave

